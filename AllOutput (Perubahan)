		if (SG.getKondisisteker().equals(g)) {
			if (SG.getPosisisteker().equals(c)
					|| (SG.getPosisisteker().equals(g))) {
				System.out.println("Keadaan Steker Sesuai");
			} else if (SG.getJumlahsteker() <= 3) {
				if (SG.getPosisisteker().equals(d)
