	if (SG.getKebisingan().equals(b)) {
		if (SG.getBau().equals(b)) {
			if (SG.getKebocoran().equals(b)) {
				if (SG.getKerusakan().equals(g)) {
					if (SG.getKeausan().equals(g)) {
					}
				}
			}
		}
	}//================================================================================================================================================
