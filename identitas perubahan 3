//import java.util.Scanner;

 public abstract class identitas implements serializable{
	 
	 	private String nama;
		private String lokasi;
		private String Fakultas;
		
		public identitas(){
			
		}
		
		public identitas(String nama, String lokasi,
				String fakultas) {
			
			this.nama = nama;
			this.lokasi = lokasi;
			this.Fakultas = fakultas;
		}
		
		
		public void setNamaRuang(String nama) {
			this.nama = nama;
		}

		public void setLokasiRuang(String lokasi) {
			this.lokasi = lokasi;
		}

		public void setFakultas(String fakultas) {
			this.Fakultas = fakultas;
		}

		public String getNamaRuang() {
			return nama;
		}
		public String getLokasiRuang() {
			return lokasi;
		}
		public String getFakultas() {
			return Fakultas;
		}

		
		abstract public String deskripsiKelas();
		
		
		

	
	 
