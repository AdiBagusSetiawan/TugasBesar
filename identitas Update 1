import java.util.Scanner;

 public class identitas extends SetGet {

	Scanner input = new Scanner(System.in);
		@Override
		void inputidentitas() {
		System.out.println("Nama Ruang : \n");
		super.setNama(input.nextLine());
		System.out.println("Lokasi Ruang : \n");
		super.setLokasi(input.nextLine());
		System.out.println("Nama Fakultas : \n");
		super.setFakultas(input.nextLine());
	}

		
	/*
	 * public void outputidenttas (){
	 * 
	 * System.out.println(""+SG.getNama());
	 * System.out.println(""+SG.getLokasi());
	 * System.out.println(""+SG.getFakultas()); }
	 */
		
	
}
