import java.util.Scanner;

public class identitas {
	SetGet SG = new SetGet();
	Scanner input = new Scanner(System.in);
	HitungRuang HR = new HitungRuang();

	public void inputidentitas() {
	
		System.out.println("Nama Ruang : \n");
		SG.setNama(input.next());
		System.out.println("Lokasi Ruang : \n");
		SG.setLokasi(input.nextLine());
		System.out.println("Fakultas : \n");
		SG.setFakultas(input.nextLine());
		
		
	}
	public void outputidenttas (){
		
		System.out.println(""+SG.getNama());
		System.out.println(""+SG.getLokasi());
		System.out.println(""+SG.getFakultas());
	}
}
